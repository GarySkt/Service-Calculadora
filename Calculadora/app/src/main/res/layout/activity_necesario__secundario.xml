<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Necesario_Secundario">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="14dp"
        android:layout_marginTop="28dp"
        android:text="@string/tV_vum"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.952" />

    <Spinner
        android:id="@+id/spinner_nivel_incidencia_musical"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="107dp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView3"
        android:layout_marginTop="80dp"
        android:text="@string/tV_Nivel_incidencia_musical"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/spinner_nivel_incidencia_musical"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView3"
        android:layout_marginTop="149dp"
        android:text="@string/tV_Aforo_local"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/eT_asientos"
        tools:layout_editor_absoluteX="16dp" />

    <EditText
        android:id="@+id/eT_asientos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView3"
        android:layout_marginTop="178dp"
        android:ems="10"
        android:hint="@string/eT_asientos"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toStartOf="@+id/textView6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/eT_asientos"
        android:layout_alignStart="@+id/tV_resultado_n_s"
        android:text="60%"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="194dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView3"
        android:layout_marginTop="236dp"
        android:text="@string/tV_horas_musica_mes"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/eT_horas_dia"
        tools:layout_editor_absoluteX="31dp" />

    <EditText
        android:id="@+id/eT_horas_dia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView3"
        android:layout_marginTop="263dp"
        android:ems="10"
        android:hint="@string/eT_horas_dia"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/eT_dias_mes"
        tools:layout_editor_absoluteX="16dp" />

    <EditText
        android:id="@+id/eT_dias_mes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/textView3"
        android:layout_marginBottom="224dp"
        android:ems="10"
        android:hint="@string/et_dias_mes"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/spinner_medio_ejecucion"
        android:layout_marginBottom="187dp"
        android:text="@string/tV_medio_ejecucion"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/spinner_medio_ejecucion"
        tools:layout_editor_absoluteX="16dp" />

    <Spinner
        android:id="@+id/spinner_medio_ejecucion"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="150dp"
        app:layout_constraintBottom_toTopOf="@+id/tV_resultado_n_s"
        tools:layout_editor_absoluteX="21dp" />

    <TextView
        android:id="@+id/tV_resultado_n_s"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="77dp"
        android:text="TextView"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="280dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/textView9"
        android:layout_marginBottom="57dp"
        android:onClick="calcular_n_s"
        android:text="@string/btn_calcular"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tV_resultado_n_s"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner_medio_ejecucion" />


</RelativeLayout>